@import url("../legenda/style.css");
@import url('perfect-scrollbar.min.css');
@import url(http://fonts.googleapis.com/css?family=Lato:400,300,300italic,700);

/* =Theme customization starts here
-------------------------------------------------------------- */
body {
	font-family: 'Lato', sans-serif;
	color: #52525f;
	font-size: 15px;
}
h1, h2, h3, h4, h5, h6 {
	letter-spacing: 0;
}
h1{
	font-size: 35px;
	font-family: 'Lato', sans-serif;
	font-weight: 300;
	text-transform: none;
	color: #202020;
	line-height: 45px;
}

h2{
	text-transform: none;
	color: #404752;
	font-family: 'Lato', sans-serif;
	font-weight: 300;
	font-size: 30px;
	margin-bottom: 10px;
}

h3{
	text-transform: none;
	color: #404752;
	font-family: 'Lato', sans-serif;
	font-weight: 700;
	font-size: 16px;
	margin-bottom: 10px;
}
h4{
	font-size: 18px;
	font-family: 'Lato', sans-serif;
	color: #525260;
	text-transform: none;
}

.fixed-header {
	background-color: #211f1f;
}
.fixed-header .menu > li > a, .main-nav .menu > li > a {
	font-family: Ubuntu, sans-serif;
	color: #fff;
	font-size: 14px;
	text-transform: none;
}

.header-type-3 .main-nav .double-border, .header-type-4 .main-nav .double-border {
	padding: 0;
}
.header-type-3 .main-nav .menu {
	width: auto !important;
	float: left;
	border-left: 1px solid #76797c;
	position: relative;
	top: 8px;
}
.header-type-3 .main-nav .menu > li, .header-type-4 .main-nav .menu > li {
	display: block;
	text-align: left;
	padding: 0 26px !important;
}

.header-type-3 .main-nav .menu > li a {
	padding: 5px 0;
	text-transform: uppercase;
	color: #8a8a8d;
	font-size: 12px;
}

.header-type-3 .main-nav .menu > li.current-menu-parent > a, .header-type-3 .main-nav .menu > li.current-menu-item > a, .header-type-4 .main-nav .menu > li.current-menu-parent > a, .header-type-4 .main-nav .menu > li.current-menu-item > a {
	color: #fff !important;
	text-decoration: none;
}

.clear {
	clear: both;
}

.header_top_right a {
	color: #fff;
	margin-left: 15px;
}

.header-wrapper {
	background: #211f1f;
}

