	padding: 40px 0;
	background: url(images/bg_body.jpg) top left repeat;
}

#home_block05 h1, #home_block06 h1 {
	color: #fff;
}

#home_block05 hr {
	width: 60px;
	margin: 0 auto 30px auto;
	border: 1px solid #49515c;
}

#home_block06 h1 {
	padding: 20px 0 0 20px;

}
#home_block06 div.content_block {
	background: #0e0e0e url(images/bg_events.png) right top no-repeat;
}

.tp-leftarrow.default::before, 
.tp-rightarrow.default::before {
	font-size: 30px;
	font-family: "Lato", sans-serif;
	line-height: 30px;
}

.tp-leftarrow.default::before {
	content: "<";
}

.tp-rightarrow.default::before {
	content: ">";
}

.tp-rightarrow.default, .tp-leftarrow.default {
	background: none;
}

#our_apts {
	height: 300px;
	overflow: scroll;
	margin: 40px 0;
	position: relative;
}
.home_apts {
	margin: 0 0 40px 0;
}

.sldr_title {
	color: #111;
	font-family: "Lato",sans-serif;
    font-size: 35px;
    font-weight: 300;
    line-height: 45px;
    text-transform: none;
    letter-spacing: 0;
}

.smooth_slider .smooth_slideri {
	max-width: 100% !important;
}
#smooth_slider_1 hr {
	width: 60px;
	margin: 0 auto 30px 0;
	border: 1px solid #c9d1dc;
}

.smooth_slider .smooth_next {
	left: 30px;
	right: auto;
	top: 90% !important;
}

.smooth_slider .smooth_prev {
	top: 90% !important;
}

.amazingcarousel-image div, .amazingcarousel-title {
	display: none !important;
}

.tab-title {
	font-family: "Lato", sans-serif !important;
	padding-left: 60px !important;
	color: #fff !important;
	border: 1px solid #666 !important;
}

.tab-title:hover {
	text-decoration: none !important;
}
.tab_dining {
	background: #141414 url(images/icon_dining.png) 15px center no-repeat;
}

.tab_activities {
	background: #141414 url(images/icon_activities.png) 15px center no-repeat;
}

.tab_events {
	background: #141414 url(images/icon_events.png) 15px center no-repeat;
}
.opened.tab_dining, .tab_dining:hover {
	background: #fff url(images/icon_dining_bw.png) 15px center no-repeat;
	color: #000 !important;
	border: 0;
}
.opened.tab_activities, .tab_activities:hover {
	background: #fff url(images/icon_activities_bw.png) 15px center no-repeat;
	color: #000 !important;
	border: 0;
}
.opened.tab_events, .tab_events:hover {
	background: #fff url(images/icon_events_bw.png) 15px center no-repeat;
	color: #000 !important;
	border: 0;
}

.tabs .tab-content {
	background: #fff;
	position: relative;
	z-index: 999;
	border: 0;
}

#footer_top {
	background: url(images/bg_body.jpg) center repeat;
	padding: 40px 0;
	color: #d6d6d6;
}

#footer_top img.alignright {
	margin: 0 0 20px 5px;
}

#footer_top a {
	color: #d6d6d6;
	font-size: 12px;
}

#home_block07 .span4 {
	background-color: #0c0c0c;
}
.home_location {
	padding: 20px 20px 20px 40px;
	background: #0c0c0c url(images/icon_location.png) 10px center no-repeat;
	text-transform: uppercase;
	color: #fff;
	font-size: 22px;
	position: relative;
	margin-bottom: 0;
}

.home_contactus {
	padding: 20px 20px 20px 40px;
	background: #0c0c0c url(images/icon_contactus.png) 10px center no-repeat;
	text-transform: uppercase;
	color: #fff;
	font-size: 22px;
	position: relative;
	margin-bottom: 0;
}

.home_tariffs {
	padding: 10px 20px 10px 40px;
	background: #c8c5c3 url(images/icon_tariffs.png) 10px center no-repeat;
	text-transform: uppercase;
	color: #211f1f;
	font-size: 16px;
	position: relative;
	margin-bottom: 0;
}

#home_location {
	padding: 0 20px 20px;
	display: none;
	-webkit-animation: fadeIn 1s;
    animation: fadeIn 1s;
}

#home_contactus {
	padding: 0 20px 20px;
	display: none;
}

#home_tariffs {
	padding: 0 20px 20px;
	display: none;
	-webkit-animation: fadeIn 1s;
    animation: fadeIn 1s;
    background: #c8c5c3;
    color: #000;
}

#change_home_location {
	background: url("images/arrow_down.png") no-repeat scroll center center rgba(0, 0, 0, 0);
    cursor: pointer;
    display: block;
    height: 30px;
    position: absolute;
    right: 20px;
    top: 20px;
    width: 30px;
}

#change_home_contactus {
	background: url("images/arrow_down.png") no-repeat scroll center center rgba(0, 0, 0, 0);
    cursor: pointer;
    display: block;
    height: 30px;
    position: absolute;
    right: 20px;
    top: 20px;
    width: 30px;
}

#change_home_tariffs {
	background: url("images/arrow_down_black.png") no-repeat scroll center center rgba(0, 0, 0, 0);
    cursor: pointer;
    display: block;
    height: 30px;
    position: absolute;
    right: 10px;
    top: 10px;
    width: 30px;
}

.rslides {
  position: relative;
  list-style: none;
  overflow: hidden;
  width: 100%;
  padding: 0;
  margin: 0;
  }

.rslides li {
  -webkit-backface-visibility: hidden;
  position: absolute;
  display: none;
  width: 100%;
  left: 0;
  top: 0;
  }

.rslides li:first-child {
  position: relative;
  display: block;
  float: left;
  }

.rslides1_nav.prev {
	content: "";
	width: 30px;
	height: 30px;
	background: url(images/arrow_left.jpg) center no-repeat;
	display: inline-block;
}
.rslides1_nav.next {
	content: "";
	width: 30px;
	height: 30px;
	background: url(images/arrow_right.jpg) center no-repeat;
	display: inline-block;
}

.rslides hr {
	width: 100px;
}

.rslides h3 {
	font-size: 20px;
	font-weight: 500;
}

.action_address {
	font-size: 14px;
	margin-top: 20px;
}

.put_me_down {
	position: absolute;
	bottom: 0;
	width: 30%;
}
.action_image {
	position: relative;
	margin-bottom: 5px;
}
.action_image p{
	margin: 0;
	padding: 0;
	height: 0;
}
.action_label {
	position: absolute;
	bottom: 10px;
	display: inline-block;
	background: rgba(59, 62, 64, 0.5);
	padding: 5px 10px;
	margin-left: 10px;
	color: #fff;
}

